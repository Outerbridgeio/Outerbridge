{
    "name": "outerbridge",
    "version": "1.0.12",
    "description": "Outerbridge Server",
    "main": "dist/index",
    "types": "dist/index.d.ts",
    "bin": {
        "outerbridge": "dist/bin/run.js"
    },
    "files": [
        "dist",
        "npm-shrinkwrap.json",
        "oclif.manifest.json",
        "oauth2.html",
        ".env"
    ],
    "oclif": {
        "bin": "outerbridge",
        "commands": "./dist/commands"
    },
    "scripts": {
        "build": "tsc && babel bin -d dist/bin && babel bin/run.ts -o dist/bin/run.js",
        "start": "babel-node -x .ts -- bin/run.ts start",
        "dev": "concurrently \"yarn watch\" \"nodemon\"",
        "oclif-dev": "babel-node -x .ts -- bin/dev.ts start",
        "postpack": "shx rm -f oclif.manifest.json",
        "prepack": "yarn build && oclif manifest && oclif readme",
        "typeorm": "typeorm-ts-node-commonjs",
        "watch": "tsc --watch",
        "version": "oclif readme && git add README.md"
    },
    "keywords": [],
    "homepage": "https://outerbridge.io",
    "author": {
        "name": "Henry Heng",
        "email": "henryheng@outerbridge.io"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "license": "SEE LICENSE IN LICENSE.md",
    "dependencies": {
        "@oclif/core": "^1.13.10",
        "axios": "^0.27.2",
        "client-oauth2": "^4.3.3",
        "cors": "^2.8.5",
        "crypto-js": "^4.1.1",
        "dotenv": "^16.0.0",
        "ethers": "^5.6.8",
        "express": "^4.17.3",
        "localtunnel": "^2.0.2",
        "moment-timezone": "^0.5.34",
        "mongodb": "^3.6.0",
        "node-abort-controller": "^3.0.1",
        "outerbridge-components": "^1.0.6",
        "outerbridge-ui": "^1.0.7",
        "pg": "^8.4.0",
        "reflect-metadata": "^0.1.13",
        "socket.io": "^4.5.1",
        "typeorm": "^0.3.6"
    },
    "devDependencies": {
        "@babel/cli": "^7.19.3",
        "@babel/node": "^7.19.1",
        "@types/cors": "^2.8.12",
        "@types/crypto-js": "^4.1.1",
        "@types/localtunnel": "^2.0.1",
        "@types/mongodb": "^3.6.0",
        "concurrently": "^7.1.0",
        "nodemon": "^2.0.15",
        "oclif": "^3",
        "shx": "^0.3.3",
        "ts-node": "^10.7.0",
        "typescript": "^4.8.4"
    }
}
